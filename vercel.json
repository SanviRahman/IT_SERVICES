{
    "builds": [
        {
            "src": "IT_SERVICES/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python 3.13.1"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "IT_SERVICES/wsgi.py"
        }
    ]
}
